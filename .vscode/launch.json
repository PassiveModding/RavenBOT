    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Debug RAVEN",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildraven",
            "program": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/Raven.dll",
            "args": ["-p", ".\\setup\\RavenLocal.json", "-d", ".\\LiteDB\\RavenDebug.db"],
            "cwd": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Debug ELO",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildelo",
            "program": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/ELO.dll",
            "args": ["-p", ".\\setup\\ELOLocal.json", "-d", ".\\LiteDB\\ELODebug.db"],
            "cwd": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Debug CLONER",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildcloner",
            "program": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/Cloner.dll",
            "args": ["-p", ".\\setup\\ClonerLocal.json", "-d", ".\\LiteDB\\CloneDebug.db"],
            "cwd": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Debug ALL",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildmain",
            "program": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/RavenBOT.dll",
            "args": ["-p", ".\\setup\\FullLocal.json", "-d", ".\\LiteDB\\FullDebug.db"],
            "cwd": "${workspaceFolder}/RavenBOT/bin/Debug/netcoreapp2.1/",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        }
    ]
}